; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ezgal"
config/description="这是基于godot.mono的一套便于开发galgame的框架."
run/main_scene="res://scene/main.tscn"
config/features=PackedStringArray("4.4", "C#", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/image="res://image/VYCMa_by_Godot_test.png"
config/icon="res://image/icon.png"

[autoload]

Global="*res://code/Global.cs"

[display]

window/size/viewport_width=1536
window/size/viewport_height=864
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="days3"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
